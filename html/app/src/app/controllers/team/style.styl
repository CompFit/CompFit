#team {
    .topSection {
        height: 175px;
        width: 100%;
        background-color: white;
        overflow-x: hidden;
        margin-bottom: 20px;
        border-bottom: 2px solid #888;

        span {
            font-family: 'Droid Sans', sans-serif;
        }

        span+span::before {
            content: ", ";
            display: inline;
        }

        img {
            width: 130px;
            height: 130px;
            float:left;
            margin-top: 20px;
        }
    }
